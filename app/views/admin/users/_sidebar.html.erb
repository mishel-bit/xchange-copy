                            <li>
                                <%=link_to 'Pending', admin_users_path(account_status: "pending"), class: "flex items-center pl-2 p-2 text-base text-gray-900 rounded-lg hover:text-secondary-500 hover:bg-secondary-50 group dark:text-gray-200 dark:hover:bg-gray-700 ml-3"%>
