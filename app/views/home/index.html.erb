<div>
    <%= render "markets_overview"%>
</div>

<div class="mt-4 mb-4 lg:mb-0">
    <h3 class="p-2 text-2xl text-gray-800 dark:text-secondary-500">
        Stocks
    <h3>
</div>
<div class="bg-white border border-gray-200 rounded-lg shadow-sm dark:border-gray-700 sm:p-6 dark:bg-gray-800">
    <div class="flex flex-col">
        <div class="overflow-x-auto rounded-lg">
            <div class="inline-block min-w-full align-middle">
                <div class="overflow-hidden shadow sm:rounded-lg">
                    <%= render "stocks_table" %>
                </div>
            </div>
        </div>
        <div class="flex items-center justify-between pt-3 sm:pt-6">
            <div>
                <span><span>
            </div>
            <div class="flex-shrink-0">
                <div class="inline-flex items-center p-2 font-medium dark:text-white">
                    <%= paginate @stocks_array %>
                </div>

            </div>
        </div>
    </div>
</div>


